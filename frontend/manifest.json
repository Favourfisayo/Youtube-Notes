{
  "manifest_version": 3,
  "name": "YouTube Note Taker",
  "description": "Generate AI notes from YouTube videos",
  "version": "1.0",
  "permissions": ["scripting", "activeTab", "storage"],
  "host_permissions": [
    "*://www.youtube.com/*",
    "http://localhost:3000/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/watch*"],
      "js": ["content.js"]
    }
  ]
}
